import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {



    static int solveMeFirst(int a, int b , int sum) {
      // Hint: Type return a+b; below
      return sum;
	}

 public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a;
        a = in.nextInt();
        int b;
        b = in.nextInt();
        int sum=a+b;
        solveMeFirst(a, b,sum);
        System.out.println(sum);
	}
}
