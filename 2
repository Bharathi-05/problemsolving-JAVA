    return sum;
    

    

}



    public static void main (String [] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i;
        int[] arr=new int[n];
        int sum=0;
        for ( i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        for(i=0;i< n; i++){
        sum=sum+arr[i];
        }
        simpleArraySum(arr,i,n,sum);
    System.out.println(sum);
    
